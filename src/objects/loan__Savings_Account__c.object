    <validationRules>
        <fullName>loan__Clients_Status_check</fullName>
        <active>false</active>
        <description>Clients_Status_check</description>
        <errorConditionFormula>NOT(ISPICKVAL( loan__Client_ID__r.loan__Status__c ,&quot;Active&quot;))</errorConditionFormula>
        <errorMessage>Client is not Approved/Active.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Deposit_Start_Date_check</fullName>
        <active>true</active>
        <description>Deposit_Start_Date_check</description>
        <errorConditionFormula>AND( (loan__Deposit_Start_Date__c &lt;  TODAY()),( PRIORVALUE(loan__Deposit_Start_Date__c)  &lt;&gt; loan__Deposit_Start_Date__c  ))</errorConditionFormula>
        <errorDisplayField>loan__Deposit_Start_Date__c</errorDisplayField>
        <errorMessage>Start Date can&apos;t be in the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Link_Current_Account_to_FD</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(ISNULL(loan__Linked_Savings_Account__c)
||
ISPICKVAL( loan__Savings_Product_Name__r.loan__Product_Type__c ,&apos;Fixed&apos;))</errorConditionFormula>
        <errorMessage>You can only link a current account with a fixed deposit. This account is not a Fixed Deposit. Please check the Savings Product Type.</errorMessage>
    </validationRules>
