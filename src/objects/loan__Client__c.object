    <validationRules>
        <fullName>loan__Address1_Special_char_check</fullName>
        <active>false</active>
        <description>Address1 Special_char_check</description>
        <errorConditionFormula>NOT(REGEX( loan__Address_1__c , &quot;^[a-zA-Z0-9-&amp;/#\\s]*$&quot;))</errorConditionFormula>
        <errorDisplayField>loan__Address_1__c</errorDisplayField>
        <errorMessage>Address should not include special characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Address2_Special_char_check</fullName>
        <active>false</active>
        <description>Address Special_char_check</description>
        <errorConditionFormula>NOT(REGEX( loan__Address_2__c , &quot;^[a-zA-Z0-9-&amp;/#\\s]*$&quot;))</errorConditionFormula>
        <errorDisplayField>loan__Address_2__c</errorDisplayField>
        <errorMessage>Address should not include special characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Address3_Special_char_check</fullName>
        <active>false</active>
        <description>Address Special_char_check</description>
        <errorConditionFormula>NOT(REGEX( loan__Address_3__c , &quot;^[a-zA-Z0-9-&amp;/#\\s]*$&quot;))</errorConditionFormula>
        <errorDisplayField>loan__Address_3__c</errorDisplayField>
        <errorMessage>Address should not include special characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Age_Between_18_and_60</fullName>
        <active>true</active>
        <errorConditionFormula>OR
      ((IF(
             MONTH(TODAY())&gt;MONTH( loan__Date_of_Birth__c ),
             YEAR(TODAY())-YEAR(loan__Date_of_Birth__c ),
             IF(AND(MONTH(TODAY()) = MONTH(loan__Date_of_Birth__c ), DAY(TODAY()) &gt;=      
             DAY(loan__Date_of_Birth__c )),YEAR(TODAY())-YEAR(loan__Date_of_Birth__c),
             (YEAR(TODAY())-YEAR(loan__Date_of_Birth__c ))-1)
             )
             &lt;  18) ,
       (IF(
            MONTH(TODAY())&gt;MONTH( loan__Date_of_Birth__c ),
            YEAR(TODAY())-YEAR(loan__Date_of_Birth__c ),
            IF(AND(MONTH(TODAY()) = MONTH(loan__Date_of_Birth__c ), DAY(TODAY()) &gt;= 
            DAY(loan__Date_of_Birth__c )),YEAR(TODAY())-YEAR(loan__Date_of_Birth__c),
           (YEAR(TODAY())-YEAR(loan__Date_of_Birth__c ))-1)
           )
           &gt;   60))</errorConditionFormula>
        <errorDisplayField>loan__Date_of_Birth__c</errorDisplayField>
        <errorMessage>Age should be between18 to 60 years</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Center_Check</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(loan__Center__c)) &amp;&amp; NOT(ISBLANK( loan__Group__c )) &amp;&amp; ( loan__Group__r.loan__Center__c !=  loan__Center__c || ISBLANK(loan__Group__r.loan__Center__c))</errorConditionFormula>
        <errorMessage>User entered Center does not match Center for Group</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Check_Max_Number_of_Persons</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( ISBLANK( loan__Group__c ) ) &amp;&amp;  (loan__Group__r.loan__Maximum_number_of_persons__c  =  loan__Group__r.loan__Number_of_Active_Clients__c )</errorConditionFormula>
        <errorMessage>You cannot add more clients to this group. The allowable number of clients has already been reached.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__City_Special_char_check</fullName>
        <active>false</active>
        <description>City_Special_char_check</description>
        <errorConditionFormula>NOT(REGEX( loan__Address_3__c , &quot;^[a-zA-Z\\s]*$&quot;))</errorConditionFormula>
        <errorDisplayField>loan__City__c</errorDisplayField>
        <errorMessage>City should not include special characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__ClientStartDate</fullName>
        <active>false</active>
        <description>This will not work here. Will only work in a trigger. Because otherwise you can never make any changes to a client! E.g. add a fee set. So deactivating for now.</description>
        <errorConditionFormula>loan__Client_Start_Date__c  &lt;    TODAY()</errorConditionFormula>
        <errorMessage>The client&apos;s start date cannot be before today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__EitherCenterOrGroupCheck</fullName>
        <active>false</active>
        <description>The client&apos;s Loan Officer and the Group&apos;s/Center&apos;s Loan Officer must be the same.</description>
        <errorConditionFormula>((loan__Group__c &lt;&gt; null) &amp;&amp; ( loan__Loan_Officer__r.Id   &lt;&gt; loan__Group__r.loan__Assigned_LO__r.Id))  || (( loan__Center__c  &lt;&gt; null) &amp;&amp; ( loan__Loan_Officer__r.Id   &lt;&gt;  loan__Center__r.loan__Loan_Officer__r.Id ))</errorConditionFormula>
