    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>loan__Check_Accounting_Type</fullName>
        <active>true</active>
        <errorConditionFormula>(loan__Accrual_Based_Accounting__c &amp;&amp;  loan__Cash_Based_Accounting__c)
||
(NOT(loan__Accrual_Based_Accounting__c) &amp;&amp;  NOT(loan__Cash_Based_Accounting__c))</errorConditionFormula>
        <errorMessage>Both accrual and cash based accounting cannot be checked or unchecked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Check_Default_Term</fullName>
        <active>true</active>
        <errorConditionFormula>OR(loan__Default_Number_of_Installments__c &lt; loan__Min_Number_of_Installments__c, loan__Default_Number_of_Installments__c &gt; loan__Max_Number_of_Installments__c)</errorConditionFormula>
        <errorMessage>Default Term is Invalid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Check_Financed_Amount_Negative</fullName>
        <active>true</active>
        <errorConditionFormula>OR(loan__Min_Loan_Amount__c &lt; 0, loan__Max_Loan_Amount__c &lt; 0)</errorConditionFormula>
        <errorMessage>Invalid Minimum and Maximum Financed Amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Check_Interest_Rate</fullName>
        <active>true</active>
        <errorConditionFormula>OR(loan__Min_Interest_Rate__c  &lt; 0, loan__Max_Interest_Rate__c &lt; 0, loan__Max_Interest_Rate__c &lt; loan__Default_Interest_Rate__c, loan__Min_Interest_Rate__c &gt; loan__Default_Interest_Rate__c )</errorConditionFormula>
        <errorMessage>Invalid Interest Rate</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Check_LOC_Product_Record_Type</fullName>
        <active>true</active>
        <errorConditionFormula>IF( (RecordType.DeveloperName == &apos;Line_Of_Credit_Product_Record_Type&apos;) &amp;&amp; (  NOT(ISPICKVAL(loan__Loan_Product_Type__c, &apos;Line of Credit&apos;))  ) , true, false)</errorConditionFormula>
        <errorMessage>Product type should be &apos;Line of Credit&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Check_Loan_Cycle_Setup_For_Amount</fullName>
        <active>false</active>
        <description>Deprecated by Abhinav and Snehal, not supported.</description>
        <errorConditionFormula>AND(  ISPICKVAL(loan__Loan_Cycle_Setup__c, &apos;Same for all Loans&apos;) ,OR( ISBLANK( loan__Max_Loan_Amount__c ) , ISBLANK( loan__Min_Loan_Amount__c ) ) )</errorConditionFormula>
        <errorMessage>Please fill out the Loan section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Check_Loan_Cycle_Setup_For_Installment</fullName>
        <active>false</active>
        <errorConditionFormula>AND(  ISPICKVAL(loan__Loan_Cycle_Setup__c, &apos;Same for all Loans&apos;) ,OR( ISBLANK(  loan__Minimum_Gap_Between_Installments__c ) , ISBLANK(  loan__Maximum_Gap_Between_Installments__c ), ISBLANK(  loan__Max_Number_of_Installments__c ) , ISBLANK(  loan__Min_Number_of_Installments__c ) , ISBLANK(  loan__Minimum_Installment_Amount__c ),  ISBLANK( loan__Default_Number_of_Installments__c )   ) )</errorConditionFormula>
        <errorMessage>Please fill out the Installment section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Check_Loan_Cycle_Setup_For_Interest</fullName>
        <active>false</active>
        <description>Deprecated by Abhinav and Snehal, not supported.</description>
        <errorConditionFormula>AND(  ISPICKVAL(loan__Loan_Cycle_Setup__c, &apos;Same for all Loans&apos;) ,OR( ISBLANK( loan__Max_Interest_Rate__c ) , ISBLANK( loan__Min_Interest_Rate__c ),ISBLANK( loan__Default_Interest_Rate__c )  ) )</errorConditionFormula>
        <errorMessage>Please fill out the Interest section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Check_Loan_Product_Record_Type</fullName>
        <active>true</active>
        <errorConditionFormula>IF( (RecordType.DeveloperName == &apos;Loan_Product_Record_Type&apos;) &amp;&amp; (  NOT(ISPICKVAL(loan__Loan_Product_Type__c, &apos;Loan&apos;))  ) , true, false)</errorConditionFormula>
        <errorMessage>Product type should be &apos;Loan&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Check_Product_Name</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Name) || ISNULL(Name)</errorConditionFormula>
        <errorMessage>Loan Product name cannot be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Check_Product_Type</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(loan__Loan_Product_Type__c,&quot;&quot;)</errorConditionFormula>
        <errorMessage>Product Type Cannot be Null</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Check_Term_Negative</fullName>
        <active>true</active>
        <errorConditionFormula>OR( loan__Min_Number_of_Installments__c &lt; 0 , loan__Max_Number_of_Installments__c &lt; 0, loan__Default_Number_of_Installments__c &lt; 0)</errorConditionFormula>
        <errorMessage>Term cannot be less than 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Check_Unique_Loan_Product</fullName>
        <active>true</active>
        <errorConditionFormula>Name = VLOOKUP( $ObjectType.loan__Loan_Product__c.Fields.Name , $ObjectType.loan__Loan_Product__c.Fields.Name , Name ) 
&amp;&amp; 
Id &lt;&gt; VLOOKUP( $ObjectType.loan__Loan_Product__c.Fields.Id , $ObjectType.loan__Loan_Product__c.Fields.Name , Name )</errorConditionFormula>
        <errorMessage>Loan Product Name already exists.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Count_Loan_Approval_Prereqs</fullName>
        <active>true</active>
        <errorConditionFormula>loan__Number_Of_Prereqs__c &gt; 1</errorConditionFormula>
        <errorMessage>You can only associate one Loan Approval Prerequisite with a Loan Product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Floating_Interest_Rate_Index_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(loan__Interest_Rate_Type__c,&quot;Floating&quot;),
     ISBLANK(loan__Floating_Rate_Index__c))</errorConditionFormula>
        <errorDisplayField>loan__Floating_Rate_Index__c</errorDisplayField>
        <errorMessage>Floating Rate Index cannot be null for a Lending Product with Interest Type &apos;Floating&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Floating_Rate_Change_Action_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(loan__Interest_Rate_Type__c,&apos;Floating&apos;),
    ISBLANK(TEXT(loan__Floating_Rate_Change_Action__c)))</errorConditionFormula>
        <errorDisplayField>loan__Floating_Rate_Change_Action__c</errorDisplayField>
        <errorMessage>Floating Rate Change Action cannot be null for a Lending Product with Interest Type &apos;Floating&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Funding_in_Tranches_in_Line_of_Credit</fullName>
        <active>true</active>
