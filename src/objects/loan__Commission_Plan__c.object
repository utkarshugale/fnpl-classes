    <validationRules>
        <fullName>loan__Flat_Rate_Validation</fullName>
        <active>true</active>
        <description>With commission calculation method as Flat ensure that factor has to be equal to 100%.</description>
        <errorConditionFormula>ISPICKVAL(loan__Calculation_Method__c , &apos;Flat Amount&apos;) &amp;&amp; (loan__Factor__c != 1.00 )</errorConditionFormula>
        <errorMessage>With commission calculation method as flat the factor has to be equal to 100%.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Validate_Rate</fullName>
        <active>true</active>
        <errorConditionFormula>((loan__Upfront_Percentage__c &gt; 1)||(loan__Maximum_Commission_Percentage__c &gt;1)||(loan__Factor__c&gt;1))</errorConditionFormula>
        <errorMessage>Upfront %, Max Commission % or Factor cannot be more than 100.00%</errorMessage>
    </validationRules>
