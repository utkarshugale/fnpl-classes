    <validationRules>
        <fullName>loan__Backdated_Payment_date_check</fullName>
        <active>false</active>
        <description>Backdated payment should not go before contract date.</description>
        <errorConditionFormula>loan__Transaction_Date__c &lt;  loan__Loan_Account__r.loan__Disbursal_Date__c</errorConditionFormula>
        <errorDisplayField>loan__Transaction_Date__c</errorDisplayField>
        <errorMessage>Transaction date should be after contract date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Check_Check_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>loan__Transaction_Amount__c !=  loan__Check__r.loan__Check_Amount__c</errorConditionFormula>
        <errorMessage>Transaction amount does not match the check amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Check_Payment_Mode</fullName>
        <active>false</active>
        <errorConditionFormula>loan__Payment_Mode__r.Name  =  &apos;Check&apos; &amp;&amp; ISBLANK( loan__Check__c )</errorConditionFormula>
        <errorMessage>Please input a check.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Cheque_Number_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>loan__Payment_Mode__r.Name  = &apos;Cheque&apos;  &amp;&amp;  ISBLANK( loan__Cheque_Number__c )</errorConditionFormula>
        <errorMessage>Cheque Number can&apos;t be blank if payment mode is Cheque.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Invalid_State_Check</fullName>
        <active>true</active>
        <errorConditionFormula>loan__Loan_Account__r.loan__Invalid_Data__c</errorConditionFormula>
        <errorMessage>Loan is in invalid state via an unexpected event. Please contact Cloud Lending support before posting this payment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Loan_Disbursal_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( loan__Loan_Account__r.loan__Disbursal_Date__c ) ||  ISNULL( loan__Loan_Account__r.loan__Disbursal_Date__c )</errorConditionFormula>
        <errorMessage>This loan is not disbursed yet. You cannot record payment transaction.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Loan_Status_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() &amp;&amp; (TEXT(loan__Loan_Account__r.loan__Loan_Status__c) = &apos;Partial Application&apos; || TEXT(loan__Loan_Account__r.loan__Loan_Status__c) = &apos;Pending Approval&apos; || TEXT(loan__Loan_Account__r.loan__Loan_Status__c) = &apos;Canceled&apos; || 
TEXT(loan__Loan_Account__r.loan__Loan_Status__c) = &apos;Closed - Rescheduled&apos; || 
TEXT(loan__Loan_Account__r.loan__Loan_Status__c) = &apos;Disbursed to LO&apos; || 
TEXT(loan__Loan_Account__r.loan__Loan_Status__c) = &apos;Partial Application&apos;)</errorConditionFormula>
        <errorMessage>Invalid Loan Status. You cannot record a repayment transaction for this loan.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Manual_Payment_Fees_Check</fullName>
        <active>false</active>
        <description>This is deprecated and not maintained.</description>
        <errorConditionFormula>ISNEW() &amp;&amp; loan__Manual_Payment__c &amp;&amp;  (ROUND(loan__Fees__c,2) &gt;   ROUND(loan__Loan_Account__r.loan__Fees_Remaining__c,2)  )</errorConditionFormula>
        <errorMessage>Fee component cannot be greater than &apos;Remaining Fees&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Manual_Payment_Principal_Check</fullName>
        <active>false</active>
        <description>This is deprecated and not maintained.</description>
        <errorConditionFormula>ISNEW() &amp;&amp; loan__Manual_Payment__c &amp;&amp;  (ROUND(loan__Principal__c,2) &gt;  ROUND(loan__Loan_Account__r.loan__Principal_Remaining__c,2) )</errorConditionFormula>
        <errorMessage>Principal component cannot be greater than &apos;Remaining Principal&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Manual_Payment_Transaction_Amount_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() &amp;&amp; loan__Manual_Payment__c &amp;&amp;  (  ROUND(loan__Transaction_Amount__c,2)   !=   ( ROUND(loan__Fees__c,2)  +  ROUND(loan__Interest__c,2)  +  ROUND(loan__Principal__c,2) + ROUND(loan__Excess__c,2))  )</errorConditionFormula>
        <errorMessage>Interest, Principal, Fees and Excess must sum up to Transaction amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Not_Write_Off_Recovery_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() &amp;&amp; 
NOT(TEXT(loan__Loan_Account__r.loan__Loan_Status__c) = &apos;Closed- Written Off&apos;) &amp;&amp;
 loan__Write_Off_Recovery_Payment__c</errorConditionFormula>
        <errorMessage>This is not a written off loan. Please uncheck the &apos;Write off Recovery&apos; option.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Transaction_Amount_Validation</fullName>
        <active>false</active>
        <description>With the addition of &apos;Excess&apos; functionality, this validation is deprecated and not maintained.</description>
        <errorConditionFormula>ISNEW() &amp;&amp; (ROUND(loan__Transaction_Amount__c,2)   &gt;   ROUND(loan__Loan_Account__r.loan__Pay_Off_Amount_As_Of_Today__c,2))</errorConditionFormula>
        <errorDisplayField>loan__Transaction_Amount__c</errorDisplayField>
        <errorMessage>Transaction Amount can not be greater than payoff amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Transaction_Date_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(loan__Transaction_Date__c &gt;  $User.loan__Current_Branch_s_System_Date__c  ,  NOT(loan__Skip_Validation__c), ISNEW())</errorConditionFormula>
        <errorMessage>Transaction/Receipt date cannot be greater than system date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Write_Off_Recovery_Amount_Check</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(loan__Loan_Account__r.loan__Loan_Status__c) = &apos;Closed- Written Off&apos; &amp;&amp;
( loan__Transaction_Amount__c &gt; loan__Loan_Account__r.loan__Principal_Remaining__c ) &amp;&amp;
ISNEW()</errorConditionFormula>
        <errorMessage>The Recovery Amount is greater than Principal Remaining.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Write_Off_Recovery_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() &amp;&amp; 
TEXT(loan__Loan_Account__r.loan__Loan_Status__c) = &apos;Closed- Written Off&apos; &amp;&amp;
 NOT(loan__Write_Off_Recovery_Payment__c)</errorConditionFormula>
        <errorMessage>This is a written off loan. You will have to select the Write Off Recovery Payment option.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Zero_Txn_Amount_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ROUND(loan__Transaction_Amount__c,2) &lt;= 0</errorConditionFormula>
        <errorMessage>Transaction amount should be greater than zero.</errorMessage>
    </validationRules>
