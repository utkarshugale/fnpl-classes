    NOT(ISBLANK(loan__Expected_Repayment_Start_Date__c)),
    NOT(ISBLANK(loan__Draw_Period_End_Date__c)),
    (loan__Expected_Repayment_Start_Date__c &lt;=  
     (loan__Draw_Period_End_Date__c +
       loan__Pre_Bill_Days__c)))</errorConditionFormula>
        <errorMessage>Payment start date should be after draw period end date + pre bill days.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Pre_Bill_Days_Value_Check</fullName>
        <active>true</active>
        <description>Pre Bill Days shouldn&apos;t be negative</description>
        <errorConditionFormula>loan__Pre_Bill_Days__c &lt;0</errorConditionFormula>
        <errorDisplayField>loan__Pre_Bill_Days__c</errorDisplayField>
        <errorMessage>&apos;Pre Bill Days&apos; can&apos;t be negative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Recurring_OT_ACH_Conflict_Check</fullName>
        <active>false</active>
        <errorConditionFormula>NOT( ISBLANK( loan__ACH_Next_Debit_Date__c ))  &amp;&amp;  NOT( ISBLANK( loan__OT_ACH_Debit_Date__c )) &amp;&amp; (loan__ACH_Next_Debit_Date__c == loan__OT_ACH_Debit_Date__c )</errorConditionFormula>
        <errorMessage>System Limitation - You cannot set up a One Time ACH and Recurring ACH transaction on the same day.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Repayment_Date_Check</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( ISNULL( loan__Expected_Repayment_Start_Date__c ) )
&amp;&amp;
loan__Expected_Repayment_Start_Date__c &lt;=
loan__Expected_Disbursal_Date__c</errorConditionFormula>
        <errorMessage>The Expected Repayment Start Date must be greater than the Expected Disbursal Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Semi_monthly_check</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(loan__Frequency_of_Loan_Payment__c  , &apos;Semi-Monthly&apos;) &amp;&amp;  (loan__Contractual_Due_Day__c == 31 )</errorConditionFormula>
        <errorMessage>Due Day For Semi-Monthly cannot be 31.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Term_Check_for_Line_of_credit</fullName>
        <active>true</active>
        <description>Validates that the Contract Term cannot be less than Draw or Repayment Term.</description>
        <errorConditionFormula>AND(ISPICKVAL(loan__Product_Type__c, &apos;Line of Credit&apos;) ,(loan__draw_term__c &gt; loan__Number_of_Installments__c))</errorConditionFormula>
        <errorMessage>Contract Term cannot be less than Draw Term</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Validate_Expected_Repayment_Start_Date</fullName>
        <active>false</active>
        <errorConditionFormula>AND( NOT(ISBLANK(loan__Expected_Repayment_Start_Date__c)) ,
loan__Expected_Repayment_Start_Date__c &lt;   loan__Branch__r.loan__Current_System_Date__c ,
 ISNEW()  )</errorConditionFormula>
        <errorMessage>Expected repayment start date cannot be less than current system date.</errorMessage>
    </validationRules>
