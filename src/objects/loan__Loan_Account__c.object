 ISCHANGED(loan__Expected_Disbursal_Date__c), loan__Expected_Disbursal_Date__c &lt;   loan__Branch__r.loan__Current_System_Date__c 
)</errorConditionFormula>
        <errorDisplayField>loan__Expected_Disbursal_Date__c</errorDisplayField>
        <errorMessage>Expected Disbursal Date cannot be less than current system date</errorMessage>
    </validationRules>
