    </validationRules>
    <validationRules>
        <fullName>loan__Client_Coaplicant_validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT($Setup.loan__Org_Parameters__c.loan__Integrate_Salesforce_CRM__c) 
,
OR(
        ( NOT(ISBLANK(loan__Co_Applicant__c)) &amp;&amp; NOT(ISBLANK(loan__Client__c )) &amp;&amp; loan__Co_Applicant__c =  loan__Client__c ),
        ( NOT(ISBLANK(loan__Guarantor__c )) &amp;&amp; NOT(ISBLANK(loan__Co_Applicant__c )) &amp;&amp; loan__Co_Applicant__c =  loan__Guarantor__c )
       )
)</errorConditionFormula>
        <errorDisplayField>loan__Co_Applicant__c</errorDisplayField>
        <errorMessage>Co-applicant can not be same as Client or Guarantor</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Client_Guarantor</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT($Setup.loan__Org_Parameters__c.loan__Integrate_Salesforce_CRM__c)
,OR(
         (  NOT(ISBLANK(loan__Guarantor__c )) &amp;&amp; NOT(ISBLANK(loan__Client__c ))  &amp;&amp; loan__Guarantor__c =  loan__Client__c ),
         (NOT(ISBLANK(loan__Guarantor__c )) &amp;&amp; NOT(ISBLANK(loan__Co_Applicant__c ))  &amp;&amp; loan__Guarantor__c =  loan__Co_Applicant__c )
      )
)</errorConditionFormula>
        <errorDisplayField>loan__Guarantor__c</errorDisplayField>
        <errorMessage>Guarantor can not be same as Client or Co-applicant.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>loan__Clients_Status_check</fullName>
        <active>false</active>
        <description>This validation rule is deprecated.</description>
        <errorConditionFormula>OR(
AND(
NOT( ISPICKVAL(loan__Loan_Product_Name__r.loan__Loan_Product_Type__c,&quot;Group&quot;)),
NOT( $Setup.loan__Org_Parameters__c.loan__Integrate_Salesforce_CRM__c ),
NOT( ISPICKVAL(loan__Client__r.loan__Status__c, &quot;Active&quot;))
),
AND(
NOT( ISPICKVAL(loan__Loan_Product_Name__r.loan__Loan_Product_Type__c,&quot;Group&quot;)),
 $Setup.loan__Org_Parameters__c.loan__Integrate_Salesforce_CRM__c ,
NOT( ISPICKVAL(loan__Account__r.loan__Active__c, &quot;Yes&quot;))
